<?xml version="1.0" encoding="UTF-8"?>
<!-- config.xml -->
<config>
	<actionsets>
		<actionset>
			<name>movetoimages</name>
			<actions>
				<action>
					<kind>move</kind>
					<destination>c:/images/</destination>
				</action>
			</actions>
		</actionset>
		<actionset>
			<name>movetodocuments</name>
			<actions>
				<action>
					<kind>move</kind>
					<destination>c:/documents/</destination>
				</action>
			</actions>
		</actionset>
		<actionset>
			<name>movetoskole</name>
			<actions>
				<action>
					<kind>move</kind>
					<destination>z:/skole/</destination>
				</action>
			</actions>
		</actionset>
	</actionsets>
	<folders>
		<folder>
			<path>c:/test/</path>
			<rulesets>
				<ruleset>
					<matchsetname>imagesregex</matchsetname>
					<actionsetname>movetoimages</actionsetname>
				</ruleset>
				<ruleset>
					<matchsetname>documentsregex</matchsetname>
					<actionsetname>movetodocuments</actionsetname>
				</ruleset>
			</rulesets>
		</folder>
		<folder>
			<path>//192.168.40.40/home/moretest/</path>
			<rulesets>
				<ruleset>
					<matchsetname>documentsregex</matchsetname>
					<actionsetname>movetoskole</actionsetname>
				</ruleset>
			</rulesets>
		</folder>
	</folders>
	<matches>
		<match>
			<name>imagesregex</name>
			<kind>regex</kind>
			<pattern>.*Image.*</pattern>
		</match>
		<match>
			<name>documentsregex</name>
			<kind>and</kind>
			<matches>
				<match>
					<kind>regex</kind>
					<pattern>.*New.*</pattern>
				</match>
				<match>
					<kind>or</kind>
					<matches>
						<match>
							<kind>extension</kind>
							<extension>.docx</extension>
						</match>
						<match>
							<kind>extension</kind>
							<extension>.txt</extension>
						</match>
					</matches>
				</match>
			</matches>
		</match>
	</matches>
</config>